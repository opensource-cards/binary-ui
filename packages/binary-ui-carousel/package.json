    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "binary-ui-components": "^0.1.0-rc.3",
    "binary-ui-icons": "^0.1.0-rc.3",
    "eslint": "^3.6.1",
